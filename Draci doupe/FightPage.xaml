<Page x:Class="Draci_doupe.FightPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Draci_doupe"
      mc:Ignorable="d" 
      d:DesignHeight="369" d:DesignWidth="642"
      Title="FightPage">

    <Grid Background="#FF59C0DE">
        <Label x:Name="label" Content="{Binding namet}" HorizontalAlignment="Left" Margin="70,56,0,0" VerticalAlignment="Top" Height="62" Width="198"/>
        <Label x:Name="label1" Content="{Binding HP}" HorizontalAlignment="Left" Margin="70,87,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="{Binding Strenght}" HorizontalAlignment="Left" Margin="70,118,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label3" Content="Label" HorizontalAlignment="Left" Margin="70,149,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label4" Content="Label" HorizontalAlignment="Left" Margin="70,180,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label5" Content="Label" HorizontalAlignment="Left" Margin="70,211,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label6" Content="Label" HorizontalAlignment="Left" Margin="70,242,0,0" VerticalAlignment="Top"/>

    </Grid>
</Page>
